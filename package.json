{
  "name": "v-onboarding",
  "version": "2.1.0",
  "description": "v-onboarding is a super-slim, fully-typed onboarding component for Vue 3",
  "repository": {
    "type": "git",
    "url": "https://github.com/fatihsolhan/v-onboarding.git"
  },
  "author": "Fatih Solhan",
  "license": "MIT",
  "private": false,
  "keywords": [
    "onboarding",
    "vue-onboarding",
    "tour",
    "vue-tour",
    "vue-introduction",
    "vue-intro",
    "vue3",
    "vue-3",
    "vue"
  ],
  "files": [
    "dist",
    "src",
    "web-types"
  ],
  "types": "./dist/types/index.d.ts",
  "main": "./dist/v-onboarding.umd.js",
  "module": "./dist/v-onboarding.es.js",
  "exports": {
    ".": {
      "import": "./dist/v-onboarding.es.js",
      "require": "./dist/v-onboarding.umd.js"
    },
    "./style": {
      "import": "./dist/style.css"
    },
    "./src": {
      "import": "./src/index.ts"
    }
  },
  "scripts": {
    "build": "vue-tsc --declaration --emitDeclarationOnly && pnpm update-web-types && vite build",
    "dev": "vite build --watch",
    "update-web-types": "vue-docgen-web-types",
    "docs:dev": "cd docs && npm run dev",
    "docs:generate": "cd docs && npm run generate",
    "demo:dev": "vite demo",
    "demo:build": "vite build demo",
    "semantic-release": "semantic-release --branches main",
    "commit": "git-cz"
  },
  "dependencies": {
    "@popperjs/core": "^2.11.6",
    "vue": "^3.2.41"
  },
  "peerDependencies": {
    "vue": "^3.2.41"
  },
  "devDependencies": {
    "@types/lodash.merge": "^4.6.7",
    "@types/node": "^18.11.9",
    "@vitejs/plugin-vue": "^3.2.0",
    "cz-conventional-changelog": "3.3.0",
    "lodash.merge": "^4.6.2",
    "minimist": "^1.2.7",
    "sass": "^1.56.0",
    "semantic-release": "^19.0.5",
    "vite": "^3.2.2",
    "vite-plugin-dts": "^1.6.6",
    "vite-plugin-vue-setup-extend": "^0.4.0",
    "vue-docgen-web-types": "^0.1.8",
    "vue-tsc": "^1.0.9"
  },
  "web-types": "web-types/web-types.json",
  "bugs": {
    "url": "https://github.com/fatihsolhan/v-onboarding/issues"
  },
  "homepage": "https://github.com/fatihsolhan/v-onboarding#readme",
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}