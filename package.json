{
  "name": "v-onboarding",
  "version": "0.0.3",
  "description": "v-onboarding is a super-slim, fully-typed onboarding plugin for Vue 3",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fatihsolhan/v-onboarding.git"
  },
  "author": "Fatih Solhan",
  "license": "MIT",
  "private": false,
  "keywords": [
    "onboarding",
    "vue-onboarding",
    "tour",
    "vue-tour",
    "vue-introduction",
    "vue-intro",
    "vue3",
    "vue-3",
    "vue"
  ],
  "files": [
    "dist",
    "src/types/*",
    "src/typings.d.ts",
    "web-types"
  ],
  "types": "src/typings.d.ts",
  "main": "./dist/v-onboarding.umd.js",
  "module": "./dist/v-onboarding.es.js",
  "exports": {
    ".": {
      "import": "./dist/v-onboarding.es.js",
      "require": "./dist/v-onboarding.umd.js"
    }
  },
  "scripts": {
    "dev": "vite playground",
    "build:playground": "vite build playground",
    "build": "yarn update-web-types && vite build",
    "update-web-types": "vue-docgen-web-types"
  },
  "dependencies": {
    "vue": "^3.2.21"
  },
  "peerDependencies": {
    "vue": "^3.2.21"
  },
  "devDependencies": {
    "@popperjs/core": "^2.11.0",
    "@rollup/plugin-typescript": "^8.3.0",
    "@types/lodash.merge": "^4.6.6",
    "@types/node": "^16.11.7",
    "@vitejs/plugin-vue": "^1.9.4",
    "autoprefixer": "^10.4.0",
    "lodash.merge": "^4.6.2",
    "minimist": "^1.2.5",
    "postcss": "^8.4.4",
    "rollup-plugin-vue": "^6.0.0",
    "sass": "^1.45.0",
    "tailwindcss": "^2.2.19",
    "tsup": "^5.7.0",
    "unbuild": "^0.5.11",
    "vite": "^2.6.14",
    "vue-docgen-web-types": "^0.1.7"
  },
  "web-types": "web-types/web-types.json",
  "bugs": {
    "url": "https://github.com/fatihsolhan/v-onboarding/issues"
  },
  "homepage": "https://github.com/fatihsolhan/v-onboarding#readme"
}
